      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label1"
          >Name Of Scheme</label
        >
        <input
          class="form-control"
          list="datalistOptions"
          id="exampleDataList"
          placeholder="Enter the name of the scheme"
        />
        <datalist id="datalistOptions">
          <% schemes.forEach(name =>{%>
            <option value="<%= name %>"></option>
          <%})%>
        </datalist>
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail2" class="form-label2"
          >Money Alloted</label
        >
        <input
          type="text"
          class="form-control"
          id="exampleInputEmail2"
          aria-describedby="emailHelp"
          placeholder="Enter the total amount given to the recipient"
        />
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail3" class="form-label3"
          >Beneficiary Name</label
        >
        <input
          type="text"
          class="form-control"
          id="exampleInputEmail3"
          aria-describedby="emailHelp"
          placeholder="Enter the name of the recipient"
        />
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail5" class="form-label5"
          >Beneficiary Phone Number</label
        >
        <input
          type="tel"
          class="form-control"
          id="exampleInputEmail5"
          aria-describedby="emailHelp"
          placeholder="Enter the contact no of the recipient"
        />
      </div>
      <label for="exampleInputEmail4" class="form-label4"
        >Beneficiary's Photo</label
      ><input
        class="form-control form-control-lg"
        id="formFileLg"
        type="file"
      />
      <button type="reset" class="btn btn-light" id="reset">Reset</button>
      <button type="submit" class="btn btn-success" id="submit">Submit</button>
    </form>

    
